{
  "name": "filter-data",
  "version": "0.1.3",
  "description": "simple, fast data filter",
  "keywords": [
    "match-sorter",
    "data filter",
    "filter data",
    "search data",
    "datatable",
    "fast search"
  ],
  "author": "ThunderMiracle",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thundermiracle/filter-data.git"
  },
  "bugs": {
    "url": "https://github.com/thundermiracle/filter-data/issues"
  },
  "homepage": "https://github.com/thundermiracle/filter-data#readme",
  "scripts": {
    "eslint:src": "eslint src/**/*.ts",
    "eslint:test": "eslint __test__/**/*.ts",
    "eslint": "yarn eslint:src && yarn eslint:test",
    "jest": "jest",
    "type-check": "tsc --noEmit --project tsconfig.eslint.json",
    "type-check:watch": "yarn type-check -- --watch",
    "test": "yarn type-check && yarn eslint && yarn jest",
    "bench": "ts-node ./benchmark/index.ts",
    "build:js:cjs": "tsc",
    "build:js:es": "tsc --project tsconfig.es.json",
    "build:js:umd": "rollup -c",
    "build:js": "yarn build:js:cjs && yarn build:js:es && yarn build:js:umd",
    "prebuild": "rimraf lib/ && rimraf dist/ && rimraf es/",
    "build": "yarn build:js"
  },
  "dependencies": {
    "ramda": "0.27.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^2.1.0",
    "@types/jest": "^25.1.4",
    "@types/match-sorter": "^4.0.0",
    "@types/node": "^13.9.1",
    "@types/ramda": "^0.26.44",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "fuse.js": "^3.4.6",
    "jest": "^25.1.0",
    "match-sorter": "^4.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.5",
    "rollup-plugin-terser": "^5.3.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
